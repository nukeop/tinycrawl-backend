  },
  "homepage": "https://github.com/nukeop/tinycrawl-backend#readme",
  "dependencies": {
    "aws-sdk": "^2.220.1",
    "basic-auth": "^2.0.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "casual": "^1.5.19",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "lodash": "^4.17.5",
    "lowdb": "^1.0.0",
    "mongodb": "^3.0.7",
